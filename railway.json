{
  "$schema": "https://railway.com/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "echo 'FORCE NIXPACKS' && bundle install"
  },
  "deploy": {
    "startCommand": "bin/rails server -b :: -p $PORT",
